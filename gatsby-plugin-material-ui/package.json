{
  "name": "gatsby-plugin-material-ui",
  "version": "4.0.0",
  "description": "Gatsby plugin for Material-UI with built-in server-side rendering support",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hupe1980/gatsby-plugin-material-ui"
  },
  "keywords": [
    "react",
    "gatsby",
    "gatsby-plugin",
    "material-ui",
    "emotion"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "gatsby-browser.js",
    "gatsby-ssr.js",
    "get-emotion-cache.js"
  ],
  "scripts": {
    "build": "babel src --out-dir .",
    "build:watch": "babel -w src --out-dir .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "lint": "eslint ./src --report-unused-disable-directives"
  },
  "dependencies": {
    "babel-preset-gatsby": "^1.0.0"
  },
  "peerDependencies": {
    "@emotion/server": "^11.4.0",
    "gatsby": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.15.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^2.3.2",
    "@emotion/server": "^11.4.0",
    "babel-preset-gatsby-package": "^1.0.0",
    "cross-env": "^7.0.3"
  }
}
